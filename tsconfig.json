{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/about.js",
    "app/(tabs)/admin/notifications.js",
    "app/(tabs)/admin/about.js",
    "app/(tabs)/admin/.js",
    "app/(tabs)/admin/index.js",
    "app/productdetail.js"
  ]
}
